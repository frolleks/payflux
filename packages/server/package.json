{
  "name": "@payflux/server",
  "description": "Payflux client library for Node.js backends",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
